---
const uwu = Astro.url.searchParams.get('uwu') === 'true'
---

{uwu ? (
  <img
    src="/images/uwugarten-square.svg"
    width={'auto'}
    height={398}
    class="h-[3rem] w-auto"
    alt="garten"
  />
) : (
  <img
    src="/images/icons/solid.svg"
    width={888}
    height={398}
    class="h-[1.8rem] w-auto"
    alt="garten"
  />
)}
