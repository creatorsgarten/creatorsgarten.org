---
import VideoActionsClient from './videoActionsClient'

export type Props = {
  slug: string
  eventId: string
  transcriptUrl?: string
}

const { eventId, slug, transcriptUrl } = Astro.props
---

<VideoActionsClient 
  eventId={eventId} 
  slug={slug} 
  transcriptUrl={transcriptUrl}
  client:load 
/>
